// Espongo le labels a livello globale
for (var k in __LABELS__) { window[k] = __LABELS__[k]; }

// Costruzione array in base alle labels presenti
var STOP_WORDS = [];
var MEX_BSERR = [];
for (var k in __LABELS__) {
    if (k.indexOf('STOP_WORDS_') == 0) STOP_WORDS.push(__LABELS__[k]);
    if (k.indexOf('MEX_BANCASELLA_') == 0) MEX_BSERR.push(k.replace('MEX_BANCASELLA_', '') + '|' + __LABELS__[k]);
}

